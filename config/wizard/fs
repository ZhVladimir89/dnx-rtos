config
        print()
        print(=========================)
        print(File system configuration)
        print(=========================)
endconfig

config
        print()
        keyread(header, ../project/flags.h, __ENABLE_DEVFS__, tmp)
        print(Do you want to enable DEVFS?)
        print(Current value: @tmp)
        additem(YES,)
        additem(NO,)
        readsel(tmp,)
        keysave(header, ../project/flags.h, __ENABLE_DEVFS__, tmp)
endconfig

